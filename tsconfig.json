{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "experimentalDecorators": true,
        "rootDir": ".",
        "sourceMap": true,
        "declaration": true,
        "listFiles": false,
        "noEmitOnError": true
    },
    "atom": {
        "rewriteTsconfig": true
    },
    "filesGlob": [
        "./**/*.ts",
        "!./**/*.d.ts",
        "./typings/**/*.d.ts",
        "!./repo/**/*.ts",
        "!./node_modules/**/*",
        "!./tmp/**/*.ts",
        "./node_modules/typescript/lib/lib.es6.d.ts",
        "./node_modules/typescript/lib/typescript.d.ts"
    ],
    "exclude": [],
    "files": [
        "./src/file/File.ts",
        "./src/file/FileIndex.ts",
        "./src/file/IFileArrDict.ts",
        "./src/file/IFileDict.ts",
        "./src/header/HeaderSuite.ts",
        "./src/index.ts",
        "./src/lint/TSLintFormatter.ts",
        "./src/lint/TSLintSuite.ts",
        "./src/lint/TSLintWorker.ts",
        "./src/reporter/DefaultReporter.ts",
        "./src/reporter/ITestReporter.ts",
        "./src/reporter/Print.ts",
        "./src/suite/ITestSuite.ts",
        "./src/suite/TestSuiteBase.ts",
        "./src/test/ITest.ts",
        "./src/test/ITestOptions.ts",
        "./src/test/TestQueue.ts",
        "./src/test/TestResult.ts",
        "./src/test/TestRunner.ts",
        "./src/tsc/EvalSuite.ts",
        "./src/tsc/ITscExecOptions.ts",
        "./src/tsc/SyntaxSuite.ts",
        "./src/tsc/Tsc.ts",
        "./src/tsc/TscparamsSuite.ts",
        "./src/tsc/TscTest.ts",
        "./src/util/exec.ts",
        "./src/util/GitChanges.ts",
        "./src/util/Timer.ts",
        "./src/util/util.ts",
        "./typings/bluebird/bluebird.d.ts",
        "./typings/bundle.d.ts",
        "./typings/chalk/chalk.d.ts",
        "./typings/findup-sync/findup-sync.d.ts",
        "./typings/fixes.d.ts",
        "./typings/git-wrapper/git-wrapper.d.ts",
        "./typings/glob/glob.d.ts",
        "./typings/lazy.js/lazy.js.d.ts",
        "./typings/minimatch/minimatch.d.ts",
        "./typings/node/node.d.ts",
        "./typings/optimist/optimist.d.ts",
        "./typings/parsimmon/parsimmon.d.ts",
        "./node_modules/typescript/lib/lib.es6.d.ts"
    ]
}
